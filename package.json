{
  "name": "inventory-ms",
  "version": "0.0.1",
  "description": "A comprehensive web-based inventory management application designed to help businesses monitor and manage their inventory efficiently.",
  "author": "Haykaz Petrosyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HaykazXP/inventory/issues"
  },
  "homepage": "https://github.com/HaykazXP/inventory#readme",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:user": "ts-node src/migration/create-user.ts",
    "migrate:products": "ts-node src/migration/create-products.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@types/handlebars": "^4.0.40",
    "bcrypt": "^5.1.0",
    "connect-mongo": "^5.0.0",
    "express-session": "^1.17.0",
    "handlebars": "^4.7.8",
    "i18next": "^23.0.0",
    "i18next-fs-backend": "^2.1.0",
    "i18next-http-middleware": "^3.5.0",
    "mongoose": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
